{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/command-line.schema.json",
  "commands": [
    {
      "name": "sortpack",
      "commandKind": "global",
      "summary": "Rush plugin for sort package.json file in the project",
      "shellCommand": "node <packageFolder>/lib/index.js",
      "safeForSimultaneousRushProcesses": true
    }
  ],
  "parameters": [
    {
      "parameterKind": "string",
      "argumentName": "FILTER",
      "longName": "--filter",
      "shortName": "-f",
      "description": "Perform a production build, including minification and localization steps",
      "associatedCommands": ["sortpack"]
    }
  ]
}
